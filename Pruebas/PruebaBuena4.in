prog sumaPrimerosDiez{
    (*
        Este programa calcula la suma de los primeros 10 números enteros.
        El exit if del for nunca se activará, es solo para comprobar que también funciona dentro del for.
    *)
    int acumulador;
    acumulador = 0;
    for (int i = 1; i <= 10; i = i + 1) {
        acumulador = acumulador + i;
        exit if acumulador < 0;
    };
    println(acumulador);
} 